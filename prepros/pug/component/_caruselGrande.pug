main.container.carousel.position-relative(style="height: 550px; margin-top: 50px;")
    .carousel.slide(data-ride="carousel" data-touch="false" data-pause="false")
        .carousel-inner
            each c in [1,2,3,4,5,6,7,8,9]
                if c == 1
                    +caruselItem("active", c) 
                else
                    +caruselItem("", c) 

                   
    include _CaruselBeneficios.pug
    